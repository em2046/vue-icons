{
  "name": "@zeit-ui/vue-icons",
  "version": "2.0.1-canary.0",
  "license": "MIT",
  "scripts": {
    "start": "vue-cli-service serve",
    "lint": "xo",
    "build": "yarn icons && vue-cli-service build",
    "icons": "node scripts/package.js",
    "build:lib": "vue-cli-service build --target lib --name index ./packages/index.js",
    "release": "yarn icons && yarn build:lib && yarn publish --access public --non-interactive",
    "now-build": "yarn build"
  },
  "main": "packages/index.js",
  "module": "packages/index.js",
  "unpkg": "dist/index.umd.js",
  "files": [
    "dist",
    "packages",
    "README.md"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "xo": {
    "extends": [
      "lambdas/vue"
    ],
    "rules": {
      "vue/no-side-effects-in-computed-properties": true,
      "prefer-object-spread": 0,
      "no-useless-escape": 0,
      "template-curly-spacing": 0,
      "import/extensions": 0
    },
    "extensions": [
      "vue",
      "js"
    ],
    "ignore": [
      "scripts",
      "packages",
      "svgs",
      "public/prism.js"
    ]
  },
  "devDependencies": {
    "@types/fs-extra": "^7.0.0",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@zeit-ui/themes": "^0.1.0",
    "@zeit-ui/vue": "^0.8.0",
    "babel-eslint": "^10.0.1",
    "core-js": "^2.6.5",
    "eslint-config-lambdas": "^0.0.11",
    "fs-extra": "^8.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue": "^2.6.10",
    "vue-auto": "^0.2.2",
    "vue-cli-plugin-pug": "^1.0.7",
    "vue-prism-component": "^1.1.1",
    "vue-template-compiler": "^2.6.10",
    "xo": "^0.24.0"
  }
}